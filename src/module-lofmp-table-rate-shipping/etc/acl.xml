<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Lof_MarketPlace::marketplace">
                    <resource id="Lof_MarketPlace::shipping" title="Shipping" sortOrder="150">
                        <resource id="Lofmp_TableRateShipping::shipping" title="Manage Table Rate Shipping" sortOrder="20"/>
                        <resource id="Lofmp_TableRateShipping::shipping_add" title="Add Table Rate Shipping" sortOrder="30"/>
                        <resource id="Lofmp_TableRateShipping::shipping_delete" title="Delete Table Rate Shipping" sortOrder="40"/>
                        <resource id="Lofmp_TableRateShipping::shipping_save" title="Save Table Rate Shipping" sortOrder="50"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
