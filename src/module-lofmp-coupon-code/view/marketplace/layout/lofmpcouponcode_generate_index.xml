<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="vendor-1column"  xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>
	    <title>Generate Coupon Code</title>
	</head>
    <update handle="seller_store_switcher"/>
    <body>
        <referenceBlock name="page.title">
            <action method="setTitleClass">
                <argument name="class" xsi:type="string">Generate Coupon Code</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Lofmp\CouponCode\Block\MarketPlace\Form\Generate\Form" name="marketplace.coupon.generation.form" template="Lofmp_CouponCode::widget/form.phtml">
            </block>
        </referenceContainer>
    </body>
</page>
