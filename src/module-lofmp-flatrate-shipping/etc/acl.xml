<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Lof_MarketPlace::marketplace" title="Lof Seller">
                    <resource id="Lof_MarketPlace::shipping" title="Shipping" sortOrder="150">
                        <resource id="Lofmp_FlatRateShipping::shipping" title="Manage Flat Rate Shipping" sortOrder="20"/>
                        <resource id="Lofmp_FlatRateShipping::shipping_add" title="Add Flat Rate Shipping" sortOrder="30"/>
                        <resource id="Lofmp_FlatRateShipping::shipping_delete" title="Delete Flat Rate Shipping" sortOrder="40"/>
                        <resource id="Lofmp_FlatRateShipping::shipping_save" title="Save Flat Rate Shipping" sortOrder="50"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
