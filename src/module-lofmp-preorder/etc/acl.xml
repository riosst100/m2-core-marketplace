<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::mppreorder" title="Marketplace Pre Order">
                    <resource id="Lofmp_PreOrder::preorder" title="Marketplace View Pre Orders" sortOrder="10">
                        <resource id="Lofmp_PreOrder::preorder_notify" title="Marketplace Notify Pre Order Items" sortOrder="10"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Lofmp_PreOrder::config_preorder" title="Marketplace PreOrder" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
