<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
    	<lofmarketplace>
    		<seller_settings>
    			<default_seller_group>1</default_seller_group>
    		</seller_settings>
            <buy_membership_page>
                <enabled>1</enabled>
                <keep_cart>1</keep_cart>
                <route>sellermembership</route>
                <page_title></page_title>
                <expiry_notification>lofmarketplace_buy_membership_page_expiry_notification</expiry_notification>
                <expiry_day_before>7</expiry_day_before>
                <sender_email_identity>general</sender_email_identity>
                <debug_mode>1</debug_mode>
            </buy_membership_page>
    	</lofmarketplace>
    </default>
</config>
