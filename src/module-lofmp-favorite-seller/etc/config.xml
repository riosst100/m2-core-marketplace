<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <favoriteseller_config>
            <general>
                <enable>1</enable>
                <show_wishlist>1</show_wishlist>
                <show_compare>1</show_compare>
                <show_cart>1</show_cart>
            </general>
            <email>
                <enable>1</enable>
                <sender_email_identity>general</sender_email_identity>
                <email_template>favoriteseller_config_email_email_template</email_template>
            </email>
        </favoriteseller_config>
    </default>
</config>