<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-favorite-sellers-link" ifconfig="favoriteseller_config/general/enable">
                <arguments>
                    <argument name="path" xsi:type="string">favoriteseller/</argument>
                    <argument name="label" xsi:type="string" translate="true">My Favorite Sellers</argument>
                    <argument name="sortOrder" xsi:type="number">200</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
